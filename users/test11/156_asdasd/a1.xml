<?xml version = '1.0' encoding = 'ISO-8859-1'?>
<!-- MLP configuration file generated by Machine Learning Platform -->
<MLPExperiment Description="description">
    <!-- wszystko zapisuje do OutputFolder: ControlFile, OutputFiles -->
    <MLPClassification OutputFolder="users/test11/out" Runs="5" LogLevel="-4">
        <MLPDatasets>
            <MLPDataset DSFormat="C4.5" DSID="1" Name="ches3x3x10000" Path2Stem="users/test11/chess3x3x10000">
                <MLPExperimentType Type="test"/>
                <MLPDSParam Name="costofattributes" Value="no"/>
                <MLPDSParam Name="costofclassification" Value="no"/>
            </MLPDataset>
        </MLPDatasets>
        <MLPClassifiers>
            <MLPClassifier CLID="21" Name="MLPDT_EA_AP">
                <MLPParam Name="parallelizationOMP" Value="no">
                    <!-- elementy -->
                    <MLPSubParam Name="mutationOMP" Value="yes"/>
                    <MLPSubParam Name="crossoverOMP" Value="yes"/>
                    <MLPSubParam Name="selectionOMP" Value="yes"/>
                    <MLPSubParam Name="postEAOMP" Value="yes"/>
                    <MLPSubParam Name="threadsOMP" Value="1"/>
                </MLPParam>
                <MLPParam Name="parallelization" Value="no">
                    <!-- mozliwosci: standard, distributed, hybrid-->
                    <MLPSubParam Name="type" Value="standard"/>
                    <!-- mozliwosci: normal, constselection-->
                    <MLPSubParam Name="selectiontype" Value="normal"/>
                    <!-- mozliwosci: normal, random-->
                    <MLPSubParam Name="sendbesttype" Value="normal"/>
                    <!-- czy m_mpiParallelInit ? 0 nie, >0 tak MC??? -->
                    <MLPSubParam Name="init" Value="normal"/>
                    <!-- mozliwosci: 1 i wiecej  co ile -->
                    <MLPSubParam Name="synchronization" Value="1"/>
                </MLPParam>
                <MLPParam Name="initializationtype" Value="dipolar"/>
                <MLPParam Name="innerdisjunction" Value="yes"/>
                <MLPParam Name="fitnesslog" Value="no"/>
                <MLPParam Name="sizeofpopulation" Value="64"/>
                <MLPParam Name="maximumiterations" Value="1000"/>
                <MLPParam Name="minimumiterations" Value="1000"/>
                <MLPParam Name="selection" Value="ranking linear selection"/>
                <MLPParam Name="probabilityofmutation" Value="80"/>
                <MLPParam Name="selectionpressure" Value="1.2"/>
                <MLPParam Name="probabilityofcrossover" Value="20"/>
                <MLPParam Name="maximumofidleiterations" Value="1000"/>
                <MLPParam Name="costofattributes" Value="no"/>
                <MLPParam Name="costofclassification" Value="no"/>
                <MLPParam Name="error" Value="retest"/>
                <!-- [ sum | division ] -->
                <MLPParam Name="fitnesscalculation" Value="sum"/>
                <MLPParam Name="maxtreesize" Value="no"/>
                <MLPParam Name="averagetreesize" Value="no"/>
                <!-- alfa * size -->
                <MLPParam Name="alfa" Value="0.001"/>                <!--0.005-->
                <!-- maximizing fitness by prunning -->
                <MLPParam Name="maximizefitness" Value="no"/>
                <MLPParam Name="mfprobability" Value="0"/>
                <!-- 0 - prob. for a single node; 1 - prob. for the whole tree -->
                <MLPParam Name="mutationtype" Value="1"/>
                <MLPParam Name="initmaxdepth" Value="3"/>
                <MLPParam Name="innercrossvalidation" Value="no">
                    <MLPSubParam Name="folds" Value="2"/>
                    <MLPSubParam Name="re-runs" Value="5"/>
                    <MLPSubParam Name="param" Value="alfa"/>
                    <MLPSubParam Name="lvalue" Value="0.001"/>
                    <MLPSubParam Name="rvalue" Value="0.1"/>
                    <MLPSubParam Name="steps" Value="10"/>
                </MLPParam>
                <MLPParam Name="threshold" Value="midpoint"/>
                <!-- enlarging the margin of the best tree -->
                <MLPParam Name="enlargethemargin" Value="no"/>
                <!-- branches swapped in random order -->
                <MLPParam Name="pc0" Value="20"/>
                <!-- subtrees swapped -->
                <MLPParam Name="pc1" Value="20"/>
                <!-- tests swapped -->
                <MLPParam Name="pc2" Value="20"/>
                <!-- tests between father and son exchanged -->
                <MLPParam Name="pc3" Value="20"/>
                <!-- tests swapped -->
                <MLPParam Name="pc4" Value="20"/>
                <!-- assymetric -->
                <MLPParam Name="pc5" Value="0"/>
                <!-- tests between father and son exchanged -->
                <MLPParam Name="pm0" Value="10"/>
                <!-- symmetric mutation -->
                <MLPParam Name="pm1" Value="10"/><!-- 20 -->
                <!-- Test in node changed -->
                <MLPParam Name="pm2" Value="10"/>
                <!-- Node changed into leaf -->
                <MLPParam Name="pm3" Value="10"/>
                <!-- threshold in real outcome test changed -->
                <MLPParam Name="pm4" Value="10"/><!-- 20 -->
                <!-- [add branche] -->
                <MLPParam Name="pm5" Value="10"/>
                <!-- [merge branches] -->
                <MLPParam Name="pm6" Value="10"/>
                <!-- [move value between branches] -->
                <MLPParam Name="pm7" Value="10"/>
                <!-- [Mixed dipol cut] -->
                <MLPParam Name="pm8" Value="10"/><!-- 0 -->
                <!-- [Pure dipol joined] -->
                <MLPParam Name="pm9" Value="10"/><!-- 0 -->
                <!-- Leaf changed into node -->
                <MLPParam Name="pml0" Value="100"/>
                <!-- Leaf expanded into subtree -->
                <MLPParam Name="pml1" Value="0"/>
            </MLPClassifier>

        </MLPClassifiers>
    </MLPClassification>
</MLPExperiment>
